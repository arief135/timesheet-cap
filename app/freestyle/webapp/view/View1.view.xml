<mvc:View
    controllerName="freestyle.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:uxap="sap.uxap"
    xmlns:layout="sap.ui.layout"
    xmlns:forms="sap.ui.layout.form"
>
    <uxap:ObjectPageLayout
        id="ObjectPageLayout"
        enableLazyLoading="true"
        beforeNavigate="onBeforeNavigate"
        useIconTabBar="true"
        showTitleInHeaderContent="true"
        upperCaseAnchorBar="false"
        sectionChange="onSectionChange"
    >
        <uxap:headerTitle>
            <uxap:ObjectPageDynamicHeaderTitle
                id="_IDGenObjectPageDynamicHeaderTitle1"
            >
                <uxap:expandedHeading>
                    <Title
                        id="_IDGenTitle1"
                        text="Timesheet"
                        wrapping="true"
                    />
                </uxap:expandedHeading>

                <uxap:snappedHeading>
                    <FlexBox
                        id="_IDGenFlexBox1"
                        fitContainer="true"
                        alignItems="Center"
                    >
                        <Avatar
                            id="_IDGenAvatar1"
                            src="./test-resources/sap/uxap/images/imageID_275314.png"
                            class="sapUiTinyMarginEnd"
                        />
                        <Title
                            id="_IDGenTitle2"
                            text="Denise Smith"
                            wrapping="true"
                        />
                    </FlexBox>
                </uxap:snappedHeading>

                <uxap:expandedContent>
                    <Text
                        id="_IDGenText1"
                        text="Senior UI Developer"
                    />
                </uxap:expandedContent>

                <uxap:snappedContent>
                    <Text
                        id="_IDGenText2"
                        text="Senior UI Developer"
                    />
                </uxap:snappedContent>

                <uxap:snappedTitleOnMobile>
                    <Title
                        id="_IDGenTitle3"
                        text="Senior UI Developer"
                    />
                </uxap:snappedTitleOnMobile>

                <uxap:actions>
                    <Button
                        id="_IDGenButton1"
                        text="Edit"
                        type="Emphasized"
                        press="onEdit"
                    />
                    <Button
                        id="_IDGenButton2"
                        type="Transparent"
                        text="Delete"
                    />
                    <Button
                        id="_IDGenButton3"
                        type="Transparent"
                        text="Copy"
                    />
                    <OverflowToolbarButton
                        id="_IDGenOverflowToolbarButton1"
                        icon="sap-icon://action"
                        type="Transparent"
                        text="Share"
                        tooltip="action"
                    />
                </uxap:actions>
            </uxap:ObjectPageDynamicHeaderTitle>
        </uxap:headerTitle>

        <uxap:headerContent>
            <FlexBox
                id="_IDGenFlexBox2"
                wrap="Wrap"
                fitContainer="true"
            >
                <Avatar
                    id="_IDGenAvatar2"
                    class="sapUiSmallMarginEnd"
                    src="./test-resources/sap/uxap/images/imageID_275314.png"
                    displaySize="L"
                />

                <layout:VerticalLayout
                    id="_IDGenVerticalLayout1"
                    class="sapUiSmallMarginBeginEnd"
                >
                    <Link
                        id="_IDGenLink1"
                        text="+33 6 4512 5158"
                    />
                    <Link
                        id="_IDGenLink2"
                        text="DeniseSmith@sap.com"
                    />
                </layout:VerticalLayout>

                <layout:HorizontalLayout
                    id="_IDGenHorizontalLayout1"
                    class="sapUiSmallMarginBeginEnd"
                >
                    <Image
                        id="_IDGenImage1"
                        src="./test-resources/sap/uxap/images/linkedin.png"
                    />
                    <Image
                        id="_IDGenImage2"
                        src="./test-resources/sap/uxap/images/Twitter.png"
                        class="sapUiSmallMarginBegin"
                    />
                </layout:HorizontalLayout>

                <layout:VerticalLayout
                    id="_IDGenVerticalLayout2"
                    class="sapUiSmallMarginBeginEnd"
                >
                    <Label
                        id="_IDGenLabel1"
                        text="Hello! I am Denise and I use UxAP"
                    />
                    <VBox id="_IDGenVBox1">
                        <Label
                            id="_IDGenLabel2"
                            text="Achieved goals"
                        />
                        <ProgressIndicator
                            id="_IDGenProgressIndicator1"
                            percentValue="30"
                            displayValue="30%"
                        />
                    </VBox>
                </layout:VerticalLayout>

                <layout:VerticalLayout
                    id="_IDGenVerticalLayout3"
                    class="sapUiSmallMarginBeginEnd"
                >
                    <Label
                        id="_IDGenLabel3"
                        text="San Jose, USA"
                    />
                </layout:VerticalLayout>
            </FlexBox>
        </uxap:headerContent>

        <uxap:sections>
            <uxap:ObjectPageSection
                titleUppercase="false"
                id="goals"
                title="2014 Goals Plan"
            >
                <uxap:subSections>
                    <uxap:ObjectPageSubSection
                        id="goalsSS1"
                        titleUppercase="false"
                    >
                        <uxap:blocks>
                            <forms:SimpleForm
                                id="_IDGenSimpleForm1"
                                editable="false"
                                layout="ColumnLayout"
                            >
                                <Label
                                    id="_IDGenLabel4"
                                    text="Evangelize the UI framework across the company"
                                />
                                <Text
                                    id="_IDGenText3"
                                    text="4 days overdue Cascaded"
                                />

                                <Label
                                    id="_IDGenLabel5"
                                    text="Get trained in development management direction"
                                />
                                <Text
                                    id="_IDGenText4"
                                    text="Due Nov 21"
                                />

                                <Label
                                    id="_IDGenLabel6"
                                    text="Mentor junior developers"
                                />
                                <Text
                                    id="_IDGenText5"
                                    text="Due Dec 31 Cascaded"
                                />
                            </forms:SimpleForm>
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>

                    <uxap:ObjectPageSubSection
                        id="_IDGenObjectPageSubSection1"
                        title="Products"
                        showTitle="false"
                    >
                        <uxap:blocks>
                            <Table
                                id="_IDGenTable1"
                                class="sapUxAPObjectPageSubSectionAlignContent"
                                width="auto"
                            >
                                <headerToolbar>
                                    <OverflowToolbar
                                        id="_IDGenOverflowToolbar1"
                                    >
                                        <Title
                                            id="_IDGenTitle4"
                                            text="Products"
                                            level="H2"
                                        />
                                        <ToolbarSpacer
                                            id="_IDGenToolbarSpacer1"
                                        />
                                        <SearchField
                                            id="_IDGenSearchField1"
                                            width="17.5rem"
                                        />
                                        <OverflowToolbarButton
                                            id="_IDGenOverflowToolbarButton2"
                                            tooltip="Sort"
                                            text="Sort"
                                            icon="sap-icon://sort"
                                        />
                                        <OverflowToolbarButton
                                            id="_IDGenOverflowToolbarButton3"
                                            tooltip="Filter"
                                            text="Filter"
                                            icon="sap-icon://filter"
                                        />
                                        <OverflowToolbarButton
                                            id="_IDGenOverflowToolbarButton4"
                                            tooltip="Group"
                                            text="Group"
                                            icon="sap-icon://group-2"
                                        />
                                        <OverflowToolbarButton
                                            id="_IDGenOverflowToolbarButton5"
                                            tooltip="Settings"
                                            text="Settings"
                                            icon="sap-icon://action-settings"
                                        />
                                    </OverflowToolbar>
                                </headerToolbar>

                                <columns>
                                    <Column id="_IDGenColumn1">
                                        <Text
                                            id="_IDGenText6"
                                            text="Document Number"
                                        />
                                    </Column>
                                    <Column
                                        id="_IDGenColumn2"
                                        minScreenWidth="Tablet"
                                        demandPopin="true"
                                    >
                                        <Text
                                            id="_IDGenText7"
                                            text="Company"
                                        />
                                    </Column>
                                    <Column
                                        id="_IDGenColumn3"
                                        minScreenWidth="Tablet"
                                        demandPopin="true"
                                    >
                                        <Text
                                            id="_IDGenText8"
                                            text="Contact Person"
                                        />
                                    </Column>
                                    <Column
                                        id="_IDGenColumn4"
                                        minScreenWidth="Tablet"
                                        demandPopin="true"
                                    >
                                        <Text
                                            id="_IDGenText9"
                                            text="Posting Date"
                                        />
                                    </Column>
                                    <Column
                                        id="_IDGenColumn5"
                                        hAlign="End"
                                    >
                                        <Text
                                            id="_IDGenText10"
                                            text="Amount (Local Currency)"
                                        />
                                    </Column>
                                </columns>

                                <items>
                                    <ColumnListItem id="_IDGenColumnListItem1">
