<mvc:View
    controllerName="freestyle.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:uxap="sap.uxap"
    xmlns:layout="sap.ui.layout"
    xmlns:forms="sap.ui.layout.form"
>
    <uxap:ObjectPageLayout
        id="ObjectPageLayout"
        enableLazyLoading="true"
        beforeNavigate="onBeforeNavigate"
        useIconTabBar="true"
        showTitleInHeaderContent="false"
        upperCaseAnchorBar="false"
        sectionChange="onSectionChange"
        showAnchorBar="false"
        showAnchorBarPopover="false"
        showEditHeaderButton="false"
        showHeaderContent="true"
        alwaysShowContentHeader="true"
        headerContentPinnable="false"
        toggleHeaderOnTitleClick="false"
    >
        <uxap:headerTitle>
            <uxap:ObjectPageDynamicHeaderTitle
                id="_IDGenObjectPageDynamicHeaderTitle1"

            >
                <uxap:expandedHeading>
                    <Title
                        id="_IDGenTitle1"
                        text="Timesheet"
                        wrapping="true"
                    />
                </uxap:expandedHeading>

                <uxap:actions>
                    <Button
                        id="_IDGenButton3"
                        type="Transparent"
                        text="Logout"
                    />
                </uxap:actions>
            </uxap:ObjectPageDynamicHeaderTitle>
        </uxap:headerTitle>

        <uxap:headerContent>
            <FlexBox
                id="_IDGenFlexBox2"
                wrap="Wrap"
                fitContainer="true"
            >
                <Avatar
                    id="_IDGenAvatar2"
                    class="sapUiSmallMarginEnd"
                    src="./test-resources/sap/uxap/images/imageID_275314.png"
                    displaySize="L"
                />

                <layout:VerticalLayout
                    id="_IDGenVerticalLayout1"
                    class="sapUiSmallMarginBeginEnd"
                >
                    <Link
                        id="_IDGenLink1"
                        text="+33 6 4512 5158"
                    />
                    <Link
                        id="_IDGenLink2"
                        text="DeniseSmith@sap.com"
                    />
                </layout:VerticalLayout>

                <layout:HorizontalLayout
                    id="_IDGenHorizontalLayout1"
                    class="sapUiSmallMarginBeginEnd"
                >
                    <Image
                        id="_IDGenImage1"
                        src="./test-resources/sap/uxap/images/linkedin.png"
                    />
                    <Image
                        id="_IDGenImage2"
                        src="./test-resources/sap/uxap/images/Twitter.png"
                        class="sapUiSmallMarginBegin"
                    />
                </layout:HorizontalLayout>

                <layout:VerticalLayout
                    id="_IDGenVerticalLayout2"
                    class="sapUiSmallMarginBeginEnd"
                >
                    <Label
                        id="_IDGenLabel1"
                        text="Hello! I am Denise and I use UxAP"
                    />
                    <VBox id="_IDGenVBox1">
                        <Label
                            id="_IDGenLabel2"
                            text="Achieved goals"
                        />
                        <ProgressIndicator
                            id="_IDGenProgressIndicator1"
                            percentValue="30"
                            displayValue="30%"
                        />
                    </VBox>
                </layout:VerticalLayout>

                <layout:VerticalLayout
                    id="_IDGenVerticalLayout3"
                    class="sapUiSmallMarginBeginEnd"
                >
                    <Label
                        id="_IDGenLabel3"
                        text="San Jose, USA"
                    />
                </layout:VerticalLayout>
            </FlexBox>
        </uxap:headerContent>

        <uxap:sections>
            <uxap:ObjectPageSection
                titleUppercase="false"
                id="goals"
                showTitle="false"
            >
                <uxap:subSections>
                    <uxap:ObjectPageSubSection
                        id="_IDGenObjectPageSubSection1"
                        showTitle="false"
                    >
                        <uxap:blocks>
                            <Table
                                id="_IDGenTable1"
                                class="sapUxAPObjectPageSubSectionAlignContent"
                                width="auto"
                                items="{ path: '/TimesheetEntry' }"
                            >
                                <headerToolbar>
                                    <Toolbar id="_IDGenToolbar1">
                                        <Title
                                            id="_IDGenTitle4"
                                            text="Draft Timesheet"
                                            level="H2"
                                        />
                                        <ToolbarSpacer
                                            id="_IDGenToolbarSpacer1"
                                        />
                                        <Button
                                            id="_IDGenButton4"
                                            text="Create Timesheet"
                                            press="onCreateTimesheetPress"
                                            type="Emphasized"
                                        />

                                        <Button
                                            id="_IDGenButton6"
                                            text="Submit Timesheet"
                                            press="onPress"
                                            enabled="false"
                                        />

                                    </Toolbar>
                                </headerToolbar>

                                <columns>
                                    <Column id="_IDGenColumn6" width="40px">
                                        <CheckBox id="_IDGenCheckBox2" />
                                    </Column>
                                    <Column id="_IDGenColumn1">
                                        <Text
                                            id="_IDGenText6"
                                            text="Date"
                                        />
                                    </Column>
                                    <Column
                                        id="_IDGenColumn2"
                                        demandPopin="true"
                                    >
                                        <Text
                                            id="_IDGenText7"
                                            text="Activity"
                                        />
                                    </Column>
                                    <Column
                                        id="_IDGenColumn3"
                                        demandPopin="true"
                                    >
                                        <Text
                                            id="_IDGenText8"
                                            text="Location"
                                        />
                                    </Column>
                                    <Column
                                        id="_IDGenColumn5"
                                        minScreenWidth="Tablet"
                                        hAlign="End"
                                    >
                                        <Text
                                            id="_IDGenText10"
                                            text="Detailed Activity"
                                        />
                                    </Column>
                                </columns>

                                <items>
                                    <ColumnListItem id="_IDGenColumnListItem1">
                                        <CheckBox id="_IDGenCheckBox1" />
                                        <Text
                                            id="_IDGenLink3"
                                            text="{date}"
                                        />
                                        <Text
                                            id="_IDGenText11"
                                            text="{atype/name}"
                                        />
                                        <Text
                                            id="_IDGenText12"
                                            text="{location/name}"
                                        />
                                        <Text
                                            id="_IDGenText13"
                                            text="{detail}"
                                        />
                                    </ColumnListItem>
                                </items>
                            </Table>
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>
        </uxap:sections>
    </uxap:ObjectPageLayout>
</mvc:View>
